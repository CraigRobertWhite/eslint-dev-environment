{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": [
            "warn"
        ]
    },
    "plugins": [
        "prettier"
    ]
}
